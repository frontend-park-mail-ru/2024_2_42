<div class="pin-wrapper">
  <select class="top-board-selector">
    <option value="">Select Board</option>
    {{#each boards}}
      <option value="{{this}}">{{this}}</option>
    {{/each}}
  </select>

  <div class="save-button-container"></div>

  <div class="pin-image">
    <img src="{{pinUrl}}" alt="Pin Image" class="pin-image" />
  </div>

  <div class="bottom-button-left"></div>
  <div class="bottom-button-right"></div>
</div>